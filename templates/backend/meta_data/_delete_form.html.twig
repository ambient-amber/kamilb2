<form method="post" action="{{ path('meta_data_delete', {'id': meta_data.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ meta_data.id) }}">
    <button class="btn btn-danger btn-sm">Delete</button>
</form>
