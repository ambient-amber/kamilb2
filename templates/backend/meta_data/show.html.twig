{% extends "backend/base.html.twig" %}

{% block moduleTitle %}Мета теги{% endblock %}
{% block moduleSubTitle %}Просмотр{% endblock %}

{% block content %}
<div class="ks-dashboard-tabbed-sidebar">
    <div class="ks-dashboard-tabbed-sidebar-widgets">
        <div class="row">
            <div class="col-lg-12">
                <table class="table">
                    <tbody>
                        <tr>
                            <th>Id</th>
                            <td>{{ meta_data.id }}</td>
                        </tr>
                        <tr>
                            <th>Url</th>
                            <td>{{ meta_data.url }}</td>
                        </tr>
                        <tr>
                            <th>Pub</th>
                            <td>{{ meta_data.pub ? 'Yes' : 'No' }}</td>
                        </tr>
                        <tr>
                            <th>IsRegexp</th>
                            <td>{{ meta_data.isRegexp ? 'Yes' : 'No' }}</td>
                        </tr>
                        <tr>
                            <th>Title</th>
                            <td>{{ meta_data.title }}</td>
                        </tr>
                        <tr>
                            <th>Description</th>
                            <td>{{ meta_data.description }}</td>
                        </tr>
                        <tr>
                            <th>KeyWords</th>
                            <td>{{ meta_data.keyWords }}</td>
                        </tr>
                    </tbody>
                </table>

                <table>
                    <tr>
                        <td>
                            <a class="btn btn-primary btn-sm" href="{{ path('meta_data_index') }}">back to list</a>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div>
{% endblock %}
